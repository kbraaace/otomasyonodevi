        "def dikdortgen_prizma_hacmi(uuznluk,genislik=10,yukseklik=5):\n",
        "\n",
        "  return uzunluk*genislik*yukseklik\n",
        "\n",
        "uzunluk=float(input(\"Uzunlugu giriniz:\"))\n",
        "\n",
        "hacim1=dikdortgen_prizma_hacmi(uzunluk)\n",
        "print(f\"Dikdortgen prizmanin hacmi(Varsayilan genislik=10,yukseklik=5):{hacim1}\")\n",
        "\n",
        "genislik=float(input(\"Genisligi giriniz:\"))\n",
        "hacim2=dikdortgen_prizma_hacmi(uzunluk,genislik=genislik)\n",
        "print(f\"Dikdortgen prizmanin hacmi(Belirtilen genislik,varsayilan yukseklik=5):{hacim2}\")\n",
        "\n",
        "yukseklik=float(input(\"Yuksekligi giriniz:\"))\n",
        "hacim3 = dikdortgen_prizma_hacmi(uzunluk,genislik=genislik,yukseklik=yukseklik)\n",
