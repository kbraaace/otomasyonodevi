    {
      "cell_type": "code",
      "source": [
        "class calisan:\n",
        "  zam_orani = 1.1\n",
        "  def __init__(self,isim,soyisim,maas):\n",
        "    self.isim = isim\n",
        "    self.soyisim = soyisim\n",
        "    self.maas = maas\n",
        "    self.email = isim + soyisim + \"@sirket.com\"\n",
        "\n",
        "  def bilgileri_goster(self):\n",
        "    return \"ad:{} soyad:{} maas:{} email:{}\".format(self.isim,self.soyisim,self.maas,self.email)\n",
        "\n",
        "calisan1 = calisan(\"ali\",\"veli\",5000)\n",
        "calisan2 = calisan(\"ahmet\",\"uzun\",6000)\n",
        "\n",
        "class yazilimci(calisan):\n",
        "  zam_orani = 1.3\n",
        "\n",
        "yazilimci1 = yazilimci(\"ayse\",\"yildiz\",7000)\n",
        "yazilimci2 = yazilimci(\"fatma\",\"yilmaz\",8000)\n",
        "print(calisan2.zam_orani)\n",
        "print(yazilimci1.zam_orani)\n",
        "print(yazilimci1.email)\n",
        "print(calisan2.bilgileri_goster())\n",
        "print(yazilimci1.bilgileri_goster())\n"
