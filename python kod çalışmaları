        "def urun_bilgileri(urun_adi, *fiyatlar, **ozellikler):\n",
        "  print(f\"Urun adi:{urun_adi}\")\n",
        "\n",
        "  if fiyatlar:\n",
        "    print(f\"Fiyat:{fiyatlar[0]}\")\n",
        "  else:\n",
        "    print(\"Fiyat bilgisi yok.\")\n",
        "\n",
        "  renk = ozellikler.get(\"renk\", \"Belirtilmemis\")\n",
        "  kategori = ozellikler.get(\"kategori\", \"Belirtilmemis\")\n",
        "\n",
        "  # Print statements for renk and kategori moved inside the function\n",
        "  print(f\"renk:{renk}\")\n",
        "  print(f\"Kategori:{kategori}\")\n",
        "\n",
        "urun_bilgileri(\"Elma\", 10, 12, 15, renk=\"Kirmizi\", kategori=\"Meyve\")\n",
        "print(\"\\n----\\n\")\n",
        "urun_bilgileri(\"Kitap\", kategori=\"Egitim\")"
