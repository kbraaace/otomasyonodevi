        "toplam = 0\n",
        "adet = 0\n",
        "\n",
        "while True:\n",
        "  sayi = int(input(\"bir sayi giriniz:\"))\n",
        "\n",
        "  if sayi == 0:\n",
        "    break\n",
        "\n",
        "  toplam += sayi\n",
        "  adet += 1\n",
        "\n",
        "  ortalama = toplam / adet\n",
        "  print(f\"girilen sayilarin ortalamasi:{ortalama}\")\n",
        "\n"
      ],
