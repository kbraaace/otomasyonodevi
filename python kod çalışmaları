      "cell_type": "code",
      "source": [
        "def istatistikler(sayilar):\n",
        "  toplam = sum(sayilar)\n",
        "  ortalama = toplam / len(sayilar)\n",
        "  maksimum = max(sayilar)\n",
        "  minimum = min(sayilar)\n",
        "  return toplam,ortalama,maksimum,minimum\n",
        "\n",
        "sayilar = [10, 20, 30 ,40 ,50]\n",
        "toplam,ortalama,maksimum,minimum = istatistikler(sayilar)\n",
        "\n",
        "print(\"Toplam:\",toplam)\n",
        "print(f\"Ortalama:\",ortalama)\n",
        "print(f\"Maksimum:\",maksimum)\n",
        "print(f\"Minimum:\",minimum)"
      ],
