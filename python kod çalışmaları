      "cell_type": "code",
      "source": [
        "filmler = {\n",
        "    1: {\n",
        "        \"ad\": \"Recep ivedik 1\",\n",
        "        \"seanslar\": {\n",
        "            \"12:00\": {\"koltuklar\": [True]*10},  # True: boş\n",
        "            \"15:00\": {\"koltuklar\": [True]*10},\n",
        "        }\n",
        "    },\n",
        "    2: {\n",
        "        \"ad\": \"Recep İvedik 2\",\n",
        "        \"seanslar\": {\n",
        "            \"13:00\": {\"koltuklar\": [True]*10},\n",
        "            \"18:00\": {\"koltuklar\": [True]*10},\n",
        "        }\n",
        "    }\n",
        "}\n",
        "\n",
        "def film_listesini_goster():\n",
        "    print(\"Mevcut Filmler:\")\n",
        "    for id, film in filmler.items():\n",
        "        print(f\"{id}. {film['ad']}\")\n",
        "\n",
        "def seanslari_goster(film_id):\n",
        "    print(f\"\\n {filmler[film_id]['ad']} için seanslar:\")\n",
        "    for saat in filmler[film_id]['seanslar']:\n",
        "        print(f\"{saat}\")\n",
        "\n",
        "def koltuklari_goster(film_id, saat):\n",
        "    koltuklar = filmler[film_id]['seanslar'][saat][\"koltuklar\"]\n",
        "    print(\"\\n Koltuklar (0: Dolu, 1: Boş):\")\n",
        "    for i, durum in enumerate(koltuklar):\n",
        "        print(f\"{i+1}: {'1' if durum else '0'}\", end=\"  \")\n",
        "    print()\n",
        "\n",
        "def rezervasyon_yap():\n",
        "    film_listesini_goster()\n",
        "    film_id = int(input(\"\\n Film numarasını seçin: \"))\n",
        "    seanslari_goster(film_id)\n",
        "    saat = input(\"Seans saatini girin (örn: 12:00): \")\n",
        "    koltuklari_goster(film_id, saat)\n",
        "\n",
        "    koltuk_no = int(input(\"Rezervasyon yapmak istediğiniz koltuk numarası: \"))\n",
        "    koltuklar = filmler[film_id]['seanslar'][saat][\"koltuklar\"]\n",
        "\n",
        "    if 1 <= koltuk_no <= len(koltuklar) and koltuklar[koltuk_no - 1]:\n",
        "        koltuklar[koltuk_no - 1] = False\n",
        "        print(\"Rezervasyon başarılı!\")\n",
        "    else:\n",
        "        print(\"Bu koltuk zaten dolu veya geçersiz bir numara.\")\n",
        "\n",
        "while True:\n",
        "    rezervasyon_yap()\n",
        "    devam = input(\"Yeni bir rezervasyon yapmak istiyor musunuz? (e/h): \")\n",
        "    if devam.lower() != 'e':\n",
        "        print(\"İyi seyirler!\")\n",
        "        break\n"
      ],
      "metadata": {
