        "import math\n",
        "a = float (input(\"birinci kenar:\"))\n",
        "b = float (input(\"ikinci kenar:\"))\n",
        "c = float (input(\"ucuncu kenar:\"))\n",
        "s = (a+b+c)/2\n",
        "alan = math.sqrt(s*(s-a)*(s-b)*(s-c))\n",
        "print(alan)"
