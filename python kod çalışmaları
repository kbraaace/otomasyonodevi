        "import math\n",
        "\n",
        "def main():\n",
        " r = float(input(\"Dairenin yaricapini giriniz:\"))\n",
        " theta = float(input(\"Merkez aciyi girin(derece):\"))\n",
        " alan = daire_dilimi_alani(r, theta)\n",
        " print(f\"Daire diliminin alani:{alan:.2f}\")\n",
        "\n",
        "def daire_dilimi_alani(r, theta):\n",
        "  alan = math.pi * r**2 * (theta / 360)\n",
        "  return alan\n",
        "\n",
        "main()"
