      "cell_type": "code",
      "source": [
        "class kisi:\n",
        "  zam_orani = 1.1\n",
        "  kisi_sayisi = 0\n",
        "  def __init__(self,isim,yas):\n",
        "    self.isim = isim\n",
        "    self.yas = yas\n",
        "    kisi.kisi_sayisi += 1\n",
        "\n",
        "  def bilgilerini_soyle(self):   #instance method\n",
        "    return f\"ad:{self.isim} yas:{self.yas}\"\n",
        "\n",
        "  @classmethod\n",
        "  def kisi_sayisini_soyle(cls):\n",
        "    return cls.kisi_sayisi\n",
        "\n",
        "  @classmethod\n",
        "  def string_ile_olustur(cls,string):\n",
        "    isim,yas = string.split(\"-\")\n",
        "    return cls(isim,yas)\n",
        "\n",
        "kisi1 = kisi(\"ali\",20)\n",
        "kisi2 = kisi(\"ahmet\",30)\n",
        "\n",
        "kisi3 = kisi.string_ile_olustur(\"ayse-25\")\n",
        "print(kisi.kisi_sayisini_soyle())\n",
        "print(kisi.kisi_sayisini_soyle())"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
